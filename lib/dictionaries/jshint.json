{
  "_environments": ["browser", "browserify", "couch", "devel", "dojo",
      "jasmine", "jquery", "mocha", "modules", "mootools", "node",
      "nonstandard", "phantom", "prototypejs", "qunit", "rhino", "shelljs",
      "typed", "worker", "wsh", "yui"],

  "bitwise": "no-bitwise",

  "camelcase": "camelcase",

  "curly": "curly",

  "eqeqeq": "eqeqeq",

  "forin": "guard-for-in",

  "funcscope": "block-scoped-var",

  "immed": {
    "_name": "wrap-iife",
    "true": [2, "any"]
  },

  "indent": "indent",

  "iterator": "no-iterator",

  "latedef": "no-use-before-define",

  "maxcomplexity": "complexity",

  "maxdepth": "max-depth",

  "maxlen": "max-len",

  "maxparams": "max-params",

  "maxstatements": "max-statements",

  "newcap": "new-cap",

  "noarg": "no-caller",

  "nocomma": "no-sequences",

  "noempty": "no-empty",

  "nonbsp": "no-irregular-whitespace",

  "nonew": "no-new",

  "notypeof": "valid-typeof",

  "quotmark": {
    "_name": "quotes",
    "true": [2, "single"]
  },

  "shadow": {
    "_name": "no-shadow",
    "true": 0,
    "false": 2,
    "inner": 2,
    "outer": [2, {"builtinGlobals": true, "hoist": "all"}]
  },

  "strict": {
    "_name": "strict",
    "true": [2, "function"]
  },

  "undef": "no-undef",

  "unused": "no-unused-vars",

  "varstmt": "no-var",

  "asi": {
    "_name": "semi",
    "true": [2, "always"]
  },

  "debug": "no-debuger",

  "eqnull": "no-eq-null",

  "evil": "no-eval",

  "expr": "no-unused-expressions",

  "globalstrict": {
    "_name": "strict",
    "true": 0,
    "_override": true
  },

  "laxbreak": "linebreak-style",

  "laxcomma": {
    "_name": "comma-style",
    "false": [2, "first"],
    "true": [2, "last"]
  },

  "loopfunc": "no-loop-func",

  "multistr": "no-multi-str",

  "noyield": "require-yield",

  "plusplus": "no-plusplus",

  "proto": "no-proto",

  "scripturl": "no-script-url",

  "sub": "dot-notation",

  "supernew": ["no-new-func", "no-new-wrappers"],

  "validthis": "no-invalid-this",

  "withstmt": "no-with"
}
